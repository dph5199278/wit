<form class="layui-form" action="/apps/report/design/columnupdate.html" id="selcubelevel">
	<input type="hidden" name="id" value="${col.id!''}">
	<input type="hidden" name="mid" value="${col.modelid!''}">
	<div class="layui-form-item">
		<label class="layui-form-label">指标名称</label>
		<div class="layui-input-inline">
			<input type="text" maxlength="200" name="title" value="${col.title!''}" required lay-verify="required" placeholder="请输入新指标名称" autocomplete="off" class="layui-input">
		</div>
	</div>
  	<div class="layui-form-button" >
		<div class="layui-button-block">
			 <button class="layui-btn layui-btn-normal"   lay-submit>立即提交</button>
		</div>
	</div>
</form>
